void TryStatement():
/*
 * Semantic check required here to make sure that at least one
 * finally/catch is present.
 */
{
 Token tk=null;
}
{
  tk="try" Block()
  CatchSequence()
  [ "finally" Block() ]
  { setComment(jjtThis,tk); }
}


void CatchSequence():
{}
{
 ( Catch() )*
}


